 private void Form1_Load(object sender, EventArgs e)
        {
            string[] aylar = new string[7];
            aylar[0] = "Mart";
            aylar[1] = "Nisan";
            aylar[2] = "Mayıs";
            aylar[3] = "Haziran";
            aylar[4] = "Temmuz";
            aylar[5] = "Ağustos";
            aylar[6] = "Eylül";
            Random r = new Random();
            int a;
            a = r.Next(0, 6);
            MessageBox.Show(aylar[a]);
        }
